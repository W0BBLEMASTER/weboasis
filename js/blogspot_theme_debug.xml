    <b:section class='main' id='main' showaddelement='yes'>
      <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog' version='1'>
        <b:includable id='main' var='top'>
          <!-- This is where the blog posts will be rendered -->
          <b:loop values='data:posts' var='post'>
            <div class='post-outer'>
              <h3 class='post-title entry-title' itemprop='name'>
                <a expr:href='data:post.url'><data:post.title/></a>
              </h3>
              <div class='post-body entry-content' expr:id='&quot;post-body-&quot; + data:post.id' itemprop='articleBody'>
                <data:post.body/>
              </div>
            </div>
          </b:loop>
        </b:includable>
      </b:widget>
    </b:section>
    
    <div id="notesWidget" class="widget">
      <div id="notesContainer" class="container-notes">
        <div class="notes-icon">
          <svg style="margin-left:0.5em;width:24px;height:24px;" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
            <path d="M20.71,7.04C21.1,6.65 21.1,6 20.71,5.63L18.37,3.29C18,2.9 17.35,2.9 16.96,3.29L15.12,5.12L18.87,8.87M3,17.25V21H6.75L17.81,9.93L14.06,6.18L3,17.25Z"/>
          </svg>
        </div>
        <textarea id="notesInput" tabindex="2" onkeydown="javascript:handleNoteInput(event);" onfocus="javascript:handleNotes(event,true);" onblur="javascript:handleNotes(event,false);"></textarea>
      </div>
    </div>
